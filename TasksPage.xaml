<Page x:Class="PDA2._0_.TasksPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PDA2._0_" xmlns:col="clr-namespace:System.Collections;assembly=mscorlib"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TasksPage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="31*"></ColumnDefinition>
            <ColumnDefinition Width="49*"></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <ListBox Name="tasksList" Background="{x:Null}" Foreground="White" FontSize="20" Margin="0,52,50,0" BorderBrush="{x:Null}" SelectionMode="Single" BorderThickness="0,0,0,0">
            
        </ListBox>

        <Frame x:Name="openTask" Grid.Column="1" NavigationUIVisibility="Hidden" Margin="0,28,0,0"/>
    </Grid>
</Page>
